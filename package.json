{
  "name": "backend",
  "version": "1.0.0",
  "description": "rest api for backend project",
  "main": "./src/app.js",
  "scripts": {
    "build": "babel src -d dist",
    "zip": "bestzip ./azilink-admin-backend-prod.zip ./src ./.environment ./package.json ./node_modules ./dist",
    "zip:dev": "bestzip ./azilink-admin-backend-dev.zip ./src ./.environment ./package.json ./node_modules ./dist",
    "build:prod": "npm run lint && npm run build && npm run zip",
    "build:dev": "npm run lint && npm run build && npm run zip:dev",
    "start": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/prod.env",
    "start:local": "nodemon --watch src/ --exec \"npm run lint && npm run build && node -r dotenv/config\" dist/app.js dotenv_config_path=.environment/local.env",
    "start:dev": "node -r dotenv/config dist/app.js dotenv_config_path=.environment/dev.env",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint ."
  },
  "author": "Janvi Anghan",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@sendgrid/mail": "^7.4.2",
    "add": "^2.0.6",
    "amazon-s3-uri": "0.1.0",
    "await-to-js": "^3.0.0",
    "aws-sdk": "^2.873.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dateformat": "^4.5.1",
    "deep-email-validator": "^0.1.18",
    "disposable-email-domains": "^1.0.57",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "express": "~4.17.1",
    "express-fileupload": "^1.2.1",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^5.2.6",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "git-last-commit": "^1.0.0",
    "http-status": "^1.5.0",
